    "file1 = open('cmn.txt', 'r', encoding='utf8')\n",
    "lines = file1.readlines()\n",
    "file1.close()\n",
    "\n",
    "for i,l in enumerate(lines):\n",
    "    t2 = l.split('\\t')\n",
    "    enter_words(t2[0][:-1], zh = t2[1][:-1])"
   ]
  },
